<script>
document.getElementById('searchBox').addEventListener('input', function () {
  const val = this.value;
  fetch('search.php?q=' + encodeURIComponent(val))
    .then(res => res.text())
    .then(html => {
      document.getElementById('gmResults').innerHTML = html;
    });
});
</script>
